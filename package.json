{
  "name": "cloudflare-nextjs-cursor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "openbuild": "node ../../packages/open-next/dist/index.js build --streaming --build-command \"npx turbo build\"",
    "clean": "rm -rf .turbo .next .open-next",
    "reset": "pnpm clean && rm -rf node_modules && pnpm install",
    "preview": "pnpm clean && opennextjs-cloudflare && wrangler dev",
    "deploy": "pnpm clean && opennextjs-cloudflare && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@opennextjs/aws": "^3.4.2",
    "@opennextjs/cloudflare": "^0.5.4",
    "next": "14.2.24",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.24",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "wrangler": "^3.110.0"
  }
}
